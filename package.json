{
  "name": "@promptx/website",
  "version": "0.1.0",
  "private": true,
  "description": "PromptX official website and documentation",
  "scripts": {
    "dev": "next dev -p 3025",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .next .turbo node_modules",
    "worker:dev": "cd workers/download && wrangler dev",
    "worker:deploy": "cd workers/download && wrangler deploy",
    "deploy:pages": "npm run build && wrangler pages deploy out --project-name=promptx",
    "deploy:worker": "npm run worker:deploy",
    "deploy:all": "npm run deploy:pages && npm run deploy:worker"
  },
  "dependencies": {
    "next": "^14.2.0",
    "nextra": "^3.2.0",
    "nextra-theme-docs": "^3.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-player": "^2.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.49",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
